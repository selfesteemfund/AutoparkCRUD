@model autopark.Models.Car

@{
    ViewData["Title"] = "Отчёт";
}

<h1>Отчёт</h1>

<div>
    <h4>об автомобилях</h4>

    <table class="table">
        <tr>
            <td>Всего автомобилей: </td>
            <td> @ViewBag.total</td>
        </tr>
        <tr>
            <td>Автомобили, выпущенные до трёх лет назад: </td>
            <td>@ViewBag.LessThanThree</td>
        </tr>
        <tr>
            <td>Автомобили, выпущенные более трёх лет назад::</td>
            <td>@ViewBag.OlderThanThree</td>
        </tr>
    </table>
    <div>
        <a asp-action="Index">Вернуться к списку</a>
    </div>
</div>
